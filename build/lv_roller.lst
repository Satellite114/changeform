 151:lvgl/src/widgets/lv_roller.c ****  */
 152:lvgl/src/widgets/lv_roller.c **** void lv_roller_set_selected(lv_obj_t * obj, uint16_t sel_opt, lv_anim_enable_t anim)
 153:lvgl/src/widgets/lv_roller.c **** {
 154:lvgl/src/widgets/lv_roller.c ****     LV_ASSERT_OBJ(obj, MY_CLASS);
 155:lvgl/src/widgets/lv_roller.c **** 
 156:lvgl/src/widgets/lv_roller.c ****     /*Set the value even if it's the same as the current value because
 157:lvgl/src/widgets/lv_roller.c ****      *if moving to the next option with an animation which was just deleted in the PRESS Call the a
 158:lvgl/src/widgets/lv_roller.c ****      *nothing will continue the animation.*/
 159:lvgl/src/widgets/lv_roller.c **** 
 160:lvgl/src/widgets/lv_roller.c ****     lv_roller_t * roller = (lv_roller_t *)obj;
 161:lvgl/src/widgets/lv_roller.c **** 
 162:lvgl/src/widgets/lv_roller.c ****     /*In infinite mode interpret the new ID relative to the currently visible "page"*/
 163:lvgl/src/widgets/lv_roller.c ****     if(roller->mode == LV_ROLLER_MODE_INFINITE) {
 164:lvgl/src/widgets/lv_roller.c ****         int32_t sel_opt_signed = sel_opt;
 165:lvgl/src/widgets/lv_roller.c ****         uint16_t page = roller->sel_opt_id / LV_ROLLER_INF_PAGES;
 166:lvgl/src/widgets/lv_roller.c **** 
 167:lvgl/src/widgets/lv_roller.c ****         /*`sel_opt` should be less than the number of options set by the user.
 168:lvgl/src/widgets/lv_roller.c ****          *If it's more then probably it's a reference from not the first page
 169:lvgl/src/widgets/lv_roller.c ****          *so normalize `sel_opt`*/
 170:lvgl/src/widgets/lv_roller.c ****         if(page != 0) {
 171:lvgl/src/widgets/lv_roller.c ****             sel_opt_signed -= page * LV_ROLLER_INF_PAGES;
 172:lvgl/src/widgets/lv_roller.c ****         }
 173:lvgl/src/widgets/lv_roller.c **** 
 174:lvgl/src/widgets/lv_roller.c ****         sel_opt = page * LV_ROLLER_INF_PAGES + sel_opt_signed;
 175:lvgl/src/widgets/lv_roller.c ****     }
 176:lvgl/src/widgets/lv_roller.c **** 
 177:lvgl/src/widgets/lv_roller.c ****     roller->sel_opt_id     = sel_opt < roller->option_cnt ? sel_opt : roller->option_cnt - 1;
 178:lvgl/src/widgets/lv_roller.c ****     roller->sel_opt_id_ori = roller->sel_opt_id;
 179:lvgl/src/widgets/lv_roller.c **** 
 180:lvgl/src/widgets/lv_roller.c ****     refr_position(obj, anim);
 181:lvgl/src/widgets/lv_roller.c **** }
