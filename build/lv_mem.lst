 258:lvgl/src/misc/lv_mem.c **** /**
 259:lvgl/src/misc/lv_mem.c ****  * Get a temporal buffer with the given size.
ARM GAS  C:\usertemp\ccVCkDv1.s 			page 6


 260:lvgl/src/misc/lv_mem.c ****  * @param size the required size
 261:lvgl/src/misc/lv_mem.c ****  */
 262:lvgl/src/misc/lv_mem.c **** void * lv_mem_buf_get(uint32_t size)
 263:lvgl/src/misc/lv_mem.c **** {
 264:lvgl/src/misc/lv_mem.c ****     if(size == 0) return NULL;
 265:lvgl/src/misc/lv_mem.c **** 
 266:lvgl/src/misc/lv_mem.c ****     MEM_TRACE("begin, getting %d bytes", size);
 267:lvgl/src/misc/lv_mem.c **** 
 268:lvgl/src/misc/lv_mem.c ****     /*Try to find a free buffer with suitable size*/
 269:lvgl/src/misc/lv_mem.c ****     int8_t i_guess = -1;
 270:lvgl/src/misc/lv_mem.c ****     for(uint8_t i = 0; i < LV_MEM_BUF_MAX_NUM; i++) {
 271:lvgl/src/misc/lv_mem.c ****         if(LV_GC_ROOT(lv_mem_buf[i]).used == 0 && LV_GC_ROOT(lv_mem_buf[i]).size >= size) {
 272:lvgl/src/misc/lv_mem.c ****             if(LV_GC_ROOT(lv_mem_buf[i]).size == size) {
 273:lvgl/src/misc/lv_mem.c ****                 LV_GC_ROOT(lv_mem_buf[i]).used = 1;
 274:lvgl/src/misc/lv_mem.c ****                 return LV_GC_ROOT(lv_mem_buf[i]).p;
 275:lvgl/src/misc/lv_mem.c ****             }
 276:lvgl/src/misc/lv_mem.c ****             else if(i_guess < 0) {
 277:lvgl/src/misc/lv_mem.c ****                 i_guess = i;
 278:lvgl/src/misc/lv_mem.c ****             }
 279:lvgl/src/misc/lv_mem.c ****             /*If size of `i` is closer to `size` prefer it*/
 280:lvgl/src/misc/lv_mem.c ****             else if(LV_GC_ROOT(lv_mem_buf[i]).size < LV_GC_ROOT(lv_mem_buf[i_guess]).size) {
 281:lvgl/src/misc/lv_mem.c ****                 i_guess = i;
 282:lvgl/src/misc/lv_mem.c ****             }
 283:lvgl/src/misc/lv_mem.c ****         }
 284:lvgl/src/misc/lv_mem.c ****     }
 285:lvgl/src/misc/lv_mem.c **** 
 286:lvgl/src/misc/lv_mem.c ****     if(i_guess >= 0) {
 287:lvgl/src/misc/lv_mem.c ****         LV_GC_ROOT(lv_mem_buf[i_guess]).used = 1;
 288:lvgl/src/misc/lv_mem.c ****         MEM_TRACE("returning already allocated buffer (buffer id: %d, address: %p)", i_guess,
 289:lvgl/src/misc/lv_mem.c ****                   LV_GC_ROOT(lv_mem_buf[i_guess]).p);
 290:lvgl/src/misc/lv_mem.c ****         return LV_GC_ROOT(lv_mem_buf[i_guess]).p;
 291:lvgl/src/misc/lv_mem.c ****     }
