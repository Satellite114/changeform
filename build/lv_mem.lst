 229:lvgl/src/misc/lv_mem.c **** /**
 230:lvgl/src/misc/lv_mem.c ****  * Give information about the work memory of dynamic allocation
 231:lvgl/src/misc/lv_mem.c ****  * @param mon_p pointer to a lv_mem_monitor_t variable,
 232:lvgl/src/misc/lv_mem.c ****  *              the result of the analysis will be stored here
 233:lvgl/src/misc/lv_mem.c ****  */
 234:lvgl/src/misc/lv_mem.c **** void lv_mem_monitor(lv_mem_monitor_t * mon_p)
 235:lvgl/src/misc/lv_mem.c **** {
 236:lvgl/src/misc/lv_mem.c ****     /*Init the data*/
 237:lvgl/src/misc/lv_mem.c ****     lv_memset(mon_p, 0, sizeof(lv_mem_monitor_t));
 238:lvgl/src/misc/lv_mem.c **** #if LV_MEM_CUSTOM == 0
 239:lvgl/src/misc/lv_mem.c ****     MEM_TRACE("begin");
 240:lvgl/src/misc/lv_mem.c **** 
 241:lvgl/src/misc/lv_mem.c ****     lv_tlsf_walk_pool(lv_tlsf_get_pool(tlsf), lv_mem_walker, mon_p);
 242:lvgl/src/misc/lv_mem.c **** 
 243:lvgl/src/misc/lv_mem.c ****     mon_p->total_size = LV_MEM_SIZE;
 244:lvgl/src/misc/lv_mem.c ****     mon_p->used_pct = 100 - (100U * mon_p->free_size) / mon_p->total_size;
 245:lvgl/src/misc/lv_mem.c ****     if(mon_p->free_size > 0) {
 246:lvgl/src/misc/lv_mem.c ****         mon_p->frag_pct = mon_p->free_biggest_size * 100U / mon_p->free_size;
 247:lvgl/src/misc/lv_mem.c ****         mon_p->frag_pct = 100 - mon_p->frag_pct;
 248:lvgl/src/misc/lv_mem.c ****     }
 249:lvgl/src/misc/lv_mem.c ****     else {
 250:lvgl/src/misc/lv_mem.c ****         mon_p->frag_pct = 0; /*no fragmentation if all the RAM is used*/
 251:lvgl/src/misc/lv_mem.c ****     }
 252:lvgl/src/misc/lv_mem.c **** 
 253:lvgl/src/misc/lv_mem.c ****     MEM_TRACE("finished");
 254:lvgl/src/misc/lv_mem.c **** #endif
 255:lvgl/src/misc/lv_mem.c **** }
